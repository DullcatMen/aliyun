@-webkit-keyframes hover
	to
		background-color #0096bb
@-webkit-keyframes sliderText
	from
		top -40px
		opacity 0
	to
		top 0
		opacity 1

.programVideo
	// background-color red
	position fixed
	right 0
	bottom 0
	width 100%
	// height 100%
	z-index -10	
		
.w1224
	width 1224px
	height 100%
	margin 0 auto
	padding 0 12px
.header
	width 100%
	height 500px
	// background-color #111
	position relative
	.slider-bg
		width 100%
		height 500px
		position absolute
		top 0
		left 0
		.bgs
			width 100%
			height 500px
			background-repeat no-repeat
			background-position center
			display none
			&.show
				display block
			&.bg-1
				background-image url(../img/bg-1.png)
				background-color rgb(4, 16, 31)
			&.bg-2
				background-image url(../img/bg-2.jpg)
				background-color rgb(40, 0, 97)
			&.bg-3
				background-image url(../img/bg-3.png)
				background-color rgb(33, 46, 72)
			&.bg-4
				background-image url(../img/bg-4.jpg)
				background-color rgb(14, 29, 58)
			&.bg-5
				background-color rgba(30,85,101,.65)
			
	.slider-box
		width 100%
		height 500px
		position absolute
		top 0
		z-index 2
		color #fff
		.slider-text
			height 100%
			display none
			position relative
			animation sliderText 1s
			&.t5
				text-align center
			&.show
				display block
			.slider-title
				font-size 48px
				font-weight 100
				margin-top 142px
				margin-bottom 15px
				line-height 72px
			.inslider-text
				font-size 20px
				font-weight 100
				line-height 1.5
				margin-bottom 57px
			.slider-btn-more
				display inline-block
				width 180px
				height 42px
				font-size 16px
				border 1px solid #fff
				line-height 40px
				text-align center
				// margin-top 20px
				&.btn-free
					background-color #00A2CA
					border-color #00A2CA

				
				
	.w1224
		// background-color red
		position relative
		.top-nav
			width 100%
			height 90px
			overflow hidden
			position relative
			z-index 3
			.logo-box
				float left
				margin-top 32px
				margin-right 72px
				.logo-aliyun
					float left
					width 94px
					height 31px
					background-image url(../img/aliyun.png)
				.logo-shujia
					float left
					width 49px
					height 19px
					background-image url(../img/shujia.png)
					margin-top 5px
					margin-left 5px
			.nav-li
				display block
				padding 0 18px
				color #fff
				text-align center
				line-height 90px
				&.left
					float left
					font-size 16px
					&:hover
						// background-color #0096bb
						animation hover 1s
						-webkit-animation-fill-mode forwards
				&.right
					float right
					font-size 14px
		.dropdown-box
			width 928px
			height 0
			overflow hidden
			position absolute
			top 90px
			left 232px
			.dropDown
				background-color #fff			
				display none
				// padding 20px 20px 0
				&.d1
					height 202px
				&.d2
					height 300px
				&.d3
					height 232px
				&.d4
					height 232px
				&.d5
					height 232px
				&.show
					display block
				.list-type1
					width 368px
					// height 54px
					// background-color red
					// border 1px solid red
					float left
					margin 20px 20px 0 20px
					.link-title
						display block
						font-size 14px
						line-height 18px
						color #333
					.desc
						font-size 12px
						line-height 15px
						color #8E8E8E
		.slider-btn
			width 100%
			position absolute
			bottom 40px
			left 0
			z-index 3
			text-align center
			.emBtns
				display inline-block
				cursor pointer
				width 16px
				height 16px
				border-radius 50%
				background-color rgba(255,255,255,.5)
				margin 0 3px
				&.active
					background-color #fff
			
